page {
    background-color: #fff;
}
.change_btn{
  margin-bottom: 20rpx;
  text-align: center;
  background-color: #ffffff;
  padding: 20rpx 0rpx;
  border-bottom: 20rpx solid #f5f5f5;
}
.item_img{
  width: 100rpx;
  height: 100rpx;
  display: block;
  text-align: center;
  margin: 20rpx auto;
}
.item_text{
  font-size: 25rpx;
  display: block;
  font-weight: 600;
}
.content1{
  background-color: #ffffff;
}
.list{
  padding: 20rpx;
  border: 0.5px solid #E6E6E6;
  position: relative;
}
.list_text{
  font-size: 30rpx;
  font-weight: 600;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  width: 290rpx;
}
.time_tips{
  position: absolute;
  top:45rpx;
  right: 20rpx;
  display: block;
  background-color: rgba(0, 0, 0, 0.6);
  font-size: 25rpx;
  color: #ffffff;
  padding: 10rpx 10rpx;
  min-width: 180rpx;
  border-radius: 10px 0px 0px 10px;
}
.list_time{
  vertical-align: middle;
  margin-left: 10rpx;
}
.load_none{
  padding: 10rpx 0;
  margin: 10rpx auto;
  text-align: center;
  color: #80848f;
  font-size: 30rpx;
  
}
.red_packet{
  position: fixed;
  right: -30rpx;
  top: 1000rpx;
  animation:move 3s 0s infinite;
  -webkit-animation:move 3s 0s infinite;
  transform-origin:bottom;
  -webkit-transform-origin:bottom;
}
@keyframes move
{
    0%, 65%{ 
      -webkit-transform:rotate(0deg);
      transform:rotate(0deg);
    }
    70% {  
      -webkit-transform:rotate(10deg);
      transform:rotate(10deg);
    }
    75% {  
      -webkit-transform:rotate(-10deg);
      transform:rotate(-10deg);
    }
    80% {  
      -webkit-transform:rotate(10deg);
      transform:rotate(10deg);
    }
    85% {  
      -webkit-transform:rotate(-10deg);
      transform:rotate(-10deg);
    }
    90% {  
      -webkit-transform:rotate(10deg);
      transform:rotate(10deg);
    }
    95% {  
      -webkit-transform:rotate(-10deg);
      transform:rotate(-10deg);
    }
    100% {  
      -webkit-transform:rotate(0deg);
      transform:rotate(0deg);
    }
}

@-webkit-keyframes move
{
    0%, 65%{ 
      -webkit-transform:rotate(0deg);
      transform:rotate(0deg);
    }
    70% {  
      -webkit-transform:rotate(10deg);
      transform:rotate(10deg);
    }
    75% {  
      -webkit-transform:rotate(-10deg);
      transform:rotate(-10deg);
    }
    80% {  
      -webkit-transform:rotate(10deg);
      transform:rotate(10deg);
    }
    85% {  
      -webkit-transform:rotate(-10deg);
      transform:rotate(-10deg);
    }
    90% {  
      -webkit-transform:rotate(10deg);
      transform:rotate(10deg);
    }
    95% {  
      -webkit-transform:rotate(-10deg);
      transform:rotate(-10deg);
    }
    100% {  
      -webkit-transform:rotate(0deg);
      transform:rotate(0deg);
    }
}