/* pages/my_message/my_message.wxss */
.list{
  box-sizing: border-box;
  width: 100vw;
  padding: 5vw;
  border-bottom: 1px solid #E6E6E6;
}
.icon{
  background: #FF1A1A;
  display: inline-block;
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.title{
  font-size: 30rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
}
.content{
  font-size: 20rpx;
  color: #727272;
  text-indent: 20rpx;
  line-height: 40rpx;
}