/* pages/dealers_create_template/dealers_create_template.wxss */
veiw{
  box-sizing: border-box;
}
.item{
  background: #fff;
}
.tem_bg{
  width: 100vw;
  height: 48vw;
  position: relative;
}
.tem_bg>image{
  width: 100%;
  height: 100%;
}
.tem_btn{
  color: #808080;
  font-size: 24rpx;
  background: #fff;
  border:1px solid #B3B3B3;
  border-radius: 10rpx;
  position: absolute;
  top: 40rpx;
  right: 30rpx;
  padding: 20rpx 30rpx;
}
.tem_set{
  display: flex;
}
.left_icon{
  width: 10vw;
  
}
.left_icon>image{
  width: 44rpx;
  height: 44rpx;
  margin-top: 24rpx;
  margin-left: 5vw;
}
.flex{
  display: flex;
  align-items: center;
  width: 95vw;
  padding: 20rpx 0;
  border-bottom: 1px solid #F5F5F5;
  font-size: 26rpx;
  color:#B3B3B3;
}
.flex>view:first-child{
  font-weight: bold;
  margin-right: 40rpx;
  color: #000;
}
.coin{
  width: 100rpx;
  color: #000000;
  margin-right: 5vw;
  margin-left: auto;
}
